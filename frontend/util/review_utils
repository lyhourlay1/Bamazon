export const fetchReviews = () => (
    $.ajax({
        method: 'get',
        url: 'api/reviews'
    })

)
export const fetchReview = (reviewId) => (
    $.ajax({
        method: 'get',
        url: `api/reviews/${reviewId}`,
    })
)

export const createReview = (review) => {
    return $.ajax({
        method: "post",
        url: '/api/reviews',
        data: { review }
    })
}